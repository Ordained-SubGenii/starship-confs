# Get editor completions based on the config shcema
"$schema" = 'https://starship.rs/config-schema.json'

format = """
$time\ 
$git_branch\
$git_status\
$username\
$hostname\
$c\
$elixir\
$elm\
$golang\
$haskell\
$java\
$julia\
$nodejs\
$nim\
$rust\
$docker_context
$directory $character"""

# Disable the blank line at the start of the prompt
# add_newline = false

# You can also replace your username with a neat symbol like ï„  to save some space
[username]
show_always = true
#style_user = "bg:#fc3d67"
style_user = "fg:#9A348e"
style_root = "fg:#e41c3f"
format = '[ î¯† $user]($style) '

[hostname]
ssh_only = true
ssh_symbol = "ï„ˆ "
#style = "bg:#fc3d67"
style = "fg:#9A348e"
format = '[ $ssh_symbol$hostname]($style) '
trim_at = "."
disabled = false

[directory]
#style = "fg:#33658A"
style = "fg:#879d89"
format = "[ $path]($style)"
truncation_length = 15
truncation_symbol = "â€¦/"

## Here is how you can shorten some long paths by text replacement
## similar to mapped_locations in Oh My Posh:

[directory.substitutions]
"Documents" = "ïœ˜ "
"Downloads" = "ï€™ "
#"Music" = "ï€ "
"Pictures" = "ï€¾ "
## Keep in mind that the order matters. For example:
## "Important Documents" = " ïœ˜ï„ª "
## will not be replaced, because "Documents" was already substituted before.
## So either put "Important Documents" before "Documents" or use the substituted version:
## "Important ïœ˜ " = " ïœ˜ï„ª "

[character]
success_symbol = '[âœ](bold green) '
error_symbol = '[âœ—](bold red) '

[c]
symbol = "î˜ "
style = "fg:#86BBD8"
format = '[$symbol($version)]($style) '

[docker_context]
symbol = "ïˆš " 
style = "fg:#06969A"
format = '[$symbol$context]($style) $path '

[elixir]
symbol = "ïƒƒ "
style = "fg:#86BBD8"
format = '[$symbol($version)]($style) '

[elm]
symbol = "ï†» "
style = "fg:#86BBD8"
format = '[$symbol($version)]($style) '

[git_branch]
symbol = "ï‚› "
always_show_remote = true
style = "fg:#ff823e"
format = '[ $symbol$branch(:$remote_branch)]($style)'

[git_status]
conflicted = "ğŸ³"
ahead = "ğŸ"
behind = "ğŸ˜°"
diverged = "ğŸ˜µ"
up_to_date = "âœ“"
untracked = "???"
stashed = "ğŸ“¦"
staged = '[++\($count\)](green)'
renamed = "NUFN"
deleted = "ğŸ—‘"

style = "fg:#ff823e"
format = '[$all_status$ahead_behind ]($style)'

[golang]
symbol = "î˜§ "
style = "fg:#86BBD8"
format = '[ $symbol($version)]($style) '

[haskell]
symbol = "Î» " 
style = "fg:#86BBD8"
format = '[ $symbol($version)]($style) '

[java]
symbol = "ïƒ´ "
style = "fg:#86BBD8"
format = '[ $symbol($version)]($style) '

[julia]
symbol = "î˜¤ "
style = "fg:#86BBD8"
format = '[ $symbol($version)]($style) '

[nodejs]
symbol = "ï‡Œ "
style = "fg:#86BBD8"
format = '[ $symbol($version)]($style) '

[nim]
symbol = "ïš¤"
style = "fg:#86BBD8"
format = '[ $symbol($version)]($style)'

[rust]
symbol = "î¨ "
style = "fg:#86BBD8"
format = '[ $symbol($version)]($style) '

[time]
disabled = false
time_format = "%R" # Hour:Minute Format
#style = "bg:#9A348E"
style = "fg:#fc3d67"
format = '[$time]($style)'

